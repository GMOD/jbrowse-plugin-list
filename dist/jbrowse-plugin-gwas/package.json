    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "flatbush": "^4.5.0",
    "mobx": "^6.13.6",
    "mobx-react": "^9.1.0",
    "prettier": "^3.8.1",
    "pretty-bytes": "^7.0.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
