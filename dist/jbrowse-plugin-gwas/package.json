    "@jbrowse/core": "^4.1.3",
    "@jbrowse/mobx-state-tree": "^5.0.0",
    "@jbrowse/plugin-linear-genome-view": "^4.1.3",
    "@jbrowse/plugin-wiggle": "^4.1.3",
    "@mui/icons-material": "7.3.7",
    "@mui/material": "^7.3.7",
    "@mui/system": "^7.3.7",
