{
  "version": "0.0.19",
  "name": "jbrowse-plugin-protein3d",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:all": "node scripts/test-versions.mjs setup && node scripts/test-versions.mjs run",
    "test:setup": "node scripts/test-versions.mjs setup",
    "test:setup:version": "node scripts/test-versions.mjs setup",
    "test:versions": "node scripts/test-versions.mjs run",
    "test:version": "node scripts/test-versions.mjs run",
    "clean": "rimraf dist",
    "format": "prettier --write .",
    "prebuild": "yarn clean",
    "start": "node esbuild.mjs --watch",
    "build": "tsc && NODE_ENV=production node esbuild.mjs",
    "build:bundle": "NODE_ENV=production node esbuild.mjs",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0",
    "pretest": "test -d .test-jbrowse-nightly || jbrowse create .test-jbrowse-nightly --nightly",
    "prepack": "yarn build",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@jbrowse/plugin-linear-genome-view": "^4.1.1",
    "@mui/icons-material": "^7.0.1",
    "clustal-js": "^2.0.11",
    "g2p_mapper": "^2.0.0",
    "molstar": "^5.6.1",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@emotion/react": "^11.13.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@fal-works/esbuild-plugin-global-externals": "^2.1.2",
    "@jbrowse/core": "^4.1.1",
    "@jbrowse/mobx-state-tree": "^5.4.2",
    "@mui/material": "^7.0.1",
    "@mui/system": "^7.0.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.9",
    "esbuild": "^0.27.0",
    "eslint": "^9.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "fp-ts": "^2.16.11",
    "mobx": "^6.0.0",
    "mobx-react": "^9.0.1",
    "prettier": "^3.8.1",
    "pretty-bytes": "^7.0.0",
    "puppeteer": "^24.36.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.1.2",
    "rxjs": "^7.0.0",
    "serve": "^14.2.5",
    "tss-react": "^4.9.4",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18"
  },
  "author": "Colin <colin.diesh@gmail.com>",
  "license": "MIT"
}
